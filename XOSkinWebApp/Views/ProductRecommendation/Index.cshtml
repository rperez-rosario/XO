@model IEnumerable<ProductViewModel>

@{
  ViewData["Title"] = "Product Recommendation";
}

<div class="text-center">
  <h1 class="display-4">Product Recommendation</h1>
  <p>
    XO Skin AI Recommendation Engine Recommends for your skin and climate:
  </p>
  @{ 
    foreach (ProductViewModel product in Model)
    {
    <p>
      <b>
        @Html.DisplayFor(modelItem => product.ProductName)
      </b>
    </p>
    }
  }
</div>